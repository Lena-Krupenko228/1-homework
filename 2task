#include <iostream>
using namespace std;

int main(){
    setlocale(LC_ALL, "rus"); 
    int a;
    cout << "Введите длину прямоугольника: ";
    cin >> a;
    int b;
    cout << "Введите ширину прямоугольника: ";
    cin >> b;
    cout << "Площадь равна " << a * b << endl;

   return 0;
            
}
