#include <iostream>

int main() {
    setlocale(LC_ALL, "RU");
    int n;
    std::cout << "Введите число n: ";
    std::cin >> n;

    std::cout << "Квадраты чисел от 1 до " << n << ":" << std::endl;
    for (int i = 1; i <= n; ++i) {
        std::cout << i << "^2 = " << i * i << std::endl;
    }

    return 0;
}
