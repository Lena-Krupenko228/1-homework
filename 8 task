#include <iostream>
#include <string>
#include <algorithm>

int main()
{
    const std::string vow = "aeiouyAEIOUY";
    int i = 0;
    std::string str;
    std::getline(std::cin, str);
    std::for_each(str.begin(), str.end(), [&i, vow](char t)
        {
            if (vow.find(t) != std::string::npos)
                ++i;
        });
    std::cout << i << '\n';
    return 0;
}
